extends ./inventario

block contenido
    if (inventarios)
        .grid-stack
            each inventario in inventarios 
                .grid-stack-item(data-gs-x='0' data-gs-y='0' data-gs-width='4' data-gs-height='2')
                    .grid-stack-item-content
                        a.inventariosText(href=`/inventario/${inventario.url}`) #{inventario.nombre}
                        h3 #{inventario.precio}
        script(type='text/javascript').
            $(function () {
            var options = {
            cellHeight: 80,
            verticalMargin: 10
            };
            $('.grid-stack').gridstack(options);
            });
