// Slide1
section.slide1
  .wrap-slick1
    .slick1
      .item-slick1.item1-slick1(style='background-image: url(images/master-slide-01.jpg);')
        .wrap-content-slide1.sizefull.flex-col-c-m.p-l-15.p-r-15.p-t-150.p-b-170
          h2.caption1-slide1.xl-text2.t-center.bo14.p-b-6.animated.visible-false.m-b-22(data-appear='fadeInUp')
